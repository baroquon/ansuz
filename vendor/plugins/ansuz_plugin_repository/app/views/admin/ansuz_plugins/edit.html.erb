<%= title "Edit Ansuz Plugin: #{@ansuz_plugin}" -%>
<% content_for :sidebar do -%>
  <%= link_to "All Ansuz Plugins", admin_ansuz_plugins_path -%>
<% end -%>
<% form_for :ansuz_plugin, :url => admin_ansuz_plugin_path(@ansuz_plugin), :html => { :method => :put } do |f| -%>
  <%= render :partial => 'form', :locals => { :f => f } -%>
  <%= submit_tag("Update Ansuz Plugin") -%> or <%= link_to "Cancel", admin_ansuz_plugins_path -%>
<% end -%>
